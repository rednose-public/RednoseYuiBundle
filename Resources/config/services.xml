<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="rednose_yui.driver.yogi.class">Rednose\YuiBundle\Driver\YogiDriver</parameter>
        <parameter key="rednose_yui.driver.yui_loader.class">Rednose\YuiBundle\Driver\YuiLoaderDriver</parameter>

        <parameter key="rednose_yui.builder.config_builder.class">Rednose\YuiBundle\Builder\ConfigBuilder</parameter>
        <parameter key="rednose_yui.builder.package_builder.class">Rednose\YuiBundle\Builder\PackageBuilder</parameter>
    </parameters>
    <services>
        <service id="rednose_yui.builder.config_builder" class="%rednose_yui.builder.config_builder.class%">
            <argument type="service" id="kernel"/>
            <argument type="service" id="rednose_yui.driver.yogi"/>
            <argument type="service" id="templating"/>
            <argument type="collection"/>
        </service>
        <service id="rednose_yui.builder.package_builder" class="%rednose_yui.builder.package_builder.class%">
            <argument type="service" id="rednose_yui.driver.yui_loader"/>
            <argument type="service" id="rednose_yui.builder.config_builder"/>
            <argument type="collection"/>
        </service>
    </services>
</container>
